<% include ../partials/header %>
<div class="container">
    <div class="row">
        <div class="col-md-3">
            <p class="lead">MovieHouse</p>
            <div class="list-group">
                <li class="list-group-item active">Info 1</li>
                <li class="list-group-item">Info 2</li>
                <li class="list-group-item">Info 3</li>
            </div>

        </div>
        <div class="col-md-9">
            <div class="thumbnail">
                <img class="img-responsive" src="http://image.tmdb.org/t/p/w300/<%= movieDetails["results"][0]["poster_path"] %>">
                <div class="caption-full">
                    <h4 class="pull-right">$9.00/night</h4>
                    <h4><a><%=movieDetails["results"][0]["title"] %></a></h4>
                    <p><%= movieDetails["results"][0]["overview"] %></p>

                </div>
            </div>

        </div>
    </div>
</div>



<% include ../partials/footer %>